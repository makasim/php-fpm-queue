{
  "name": "makasim/php-fpm-queue",
  "type": "library",
  "description": "Use php-fpm as a simple built-in async queue",
  "keywords": ["messaging", "queue", "fpm", "cgi"],
  "license": "MIT",
  "require": {
    "php": "^7.1.3 | ^8.0",
    "hollodotme/fast-cgi-client": "^3",
    "queue-interop/queue-interop": "^0.8.1",
    "enqueue/dsn": "^0.9 | ^0.10.8"
  },
  "autoload": {
    "psr-4": { "Makasim\\PhpFpm\\": "src/" }
  },
  "minimum-stability": "dev",
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}
